    },
    "Avatar": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": false
    },
    "password": {
      "type": "password"
    },
    "email": {
      "type": "email"
    }
  }
}
